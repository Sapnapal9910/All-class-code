package statedemo;

public class Ifcondition3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int first = 500;
	     int second = 700;
	     int third = 800;
	     
	     if(first > second)
	     {
	         	 
	     	 if(first > third)
	          {
	        	  System.out.println("Greatest : "+ first);
	          }
	     }
if(second>third)
	     {
	    	 if(second>first)
	    	 {
	    	      System.out.println("Greatest : "+ second);      		 
	    		    		 
	    	 }
	     }
	     if(third>first)
	     {
	    	 if(third >second)
	    	 {
	    	      System.out.println("Greatest : "+ third);      		 
	    		    		 
	    	 }
	     }
	}

}
